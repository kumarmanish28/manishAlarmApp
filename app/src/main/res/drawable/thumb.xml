<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/checked_thumb" />
            <stroke android:width="@dimen/_10sdp" android:color="@color/checked_track" />
            <size android:width="@dimen/_30sdp" android:height="@dimen/_30sdp" />


        </shape>


    </item>


    <item android:state_checked="false">
        <shape android:shape="oval">
            <solid android:color="@color/unchecked_thumb" />
            <stroke android:width="@dimen/_10sdp" android:color="@color/unchecked_track" />
            <size android:width="@dimen/_30sdp" android:height="@dimen/_30sdp" />


        </shape>


    </item>
</selector>